auto-fu-activate () {
	afu_paused_p=0 
}
with-afu-compfuncs () {
	compprefuncs=(afu-comppre) 
	comppostfuncs=(afu-comppost) 
	"$@"
}
afu-recursive-edit-and-accept () {
	local -a __accepted
	zle recursive-edit -K afu || {
		afu-reset
		zle -R ''
		zle send-break
		return
	}
	[[ -n ${__accepted} ]] && (( ${#${(M)afu_accept_lines:#${__accepted[1]}}} > 1 )) && {
		zle "${__accepted[@]}"
	} || {
		zle accept-line
	}
}
afu-autoable-skip-pred () {
	local place="$1" 
	local style="$2" 
	local deffn="${3-}" 
	local value="${(P)place}" 
	local -a skips
	skips=() 
	zstyle -a ':auto-fu:var' "$style" skips
	(($#skips==0)) && [[ -n "$deffn" ]] && {
		"$deffn" skips
	}
	local skip
	for skip in $skips
	do
		[[ "${value}" = ${~skip} ]] && {
			[[ -n "${AUTO_FU_DEBUG-}" ]] && {
				echo "***BREAK*** ${skip}" >> ${AUTO_FU_DEBUG-}
			}
			return 1
		}
	done
	return 0
}
